<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<PublishDir>bin\Release\net8.0-android\publish\</PublishDir>
		<PublishProtocol>FileSystem</PublishProtocol>
		<RuntimeIdentifier>win10-x86</RuntimeIdentifier>
		<Platform>Any CPU</Platform>
		<Configuration>Release</Configuration>
		<TargetFramework>net8.0-windows10.0.19041.0</TargetFramework>
		<PublishSingleFile>false</PublishSingleFile>
		<PublishReadyToRun>false</PublishReadyToRun>
		<SelfContained>True</SelfContained>
		<PublishAppxPackage>true</PublishAppxPackage>
		<AppxPackageDir>C:\Users\Administrator\Documents\G-FragUWP\</AppxPackageDir>

		<!-- Define dynamic properties for the MSIX file name -->
		<PackageName>MyApp</PackageName>
		<PackageVersion>1.0.0</PackageVersion>
		<PackageArchitecture>x64</PackageArchitecture>
		<PublisherID>MyCompany</PublisherID>
		<MsixFileName>$(PackageName)_$(PackageVersion)_$(PackageArchitecture)_$(PublisherID).msix</MsixFileName>

	</PropertyGroup>

</Project>